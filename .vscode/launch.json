{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "cds run",
      "program": "${workspaceFolder}/node_modules/.bin/ts-node",
      "request": "launch",
      "runtimeArgs": [
        "--nolazy",
        "-r",
        "ts-node/register/transpile-only"
      ],
      "args": [
        "${workspaceFolder}/node_modules/.bin/cds-ts",
        "run",
        "--with-mocks",
        "--in-memory?"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    }
  ]
}
